<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="UTF-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge" />
    <meta name="viewport" content="width=device-width, initial-scale=1.0" />
    <link rel="stylesheet" href="../public/style.css" />
    <title>Accueil</title>
  </head>
  <body>
    <div class="container">
      
      <%- include("../partial/header.ejs") %>
      <div class="all-users">
        <table>
          <thead>
            <th>Nom</th>
            <th>Prenom</th>
            <th>Telephone</th>
            <th>Email</th>
            <th>Action</th>
          </thead>
          <tbody>
            <% students.forEach(student => { %>
            <tr>
              <td><%= student.nom %></td>
              <td><%= student.prenom %></td>
              <td><%= student.tel %></td>
              <td><%= student.email %></td>
              <td>
                <a href="" class="edit">Edit</a>
                <a href="<%= student._id %>" class="delete">Delete</a>
              </td>
            </tr>
            <%})%>
          </tbody>
        </table>
      </div>
    </div>
  </body>
</html>
